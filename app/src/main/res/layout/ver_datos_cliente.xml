<?xml version="1.0" encoding="utf-8"?>
<TableLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/tableLayoutcliente"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:background="@drawable/fondoitem1">

    <TableRow
        android:layout_width="230dp"
        android:layout_height="wrap_content">

        <TextView
            android:id="@+id/textView27"
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:fontFamily="@font/francois_one"
            android:paddingStart="10dp"
            android:text="IdCliente:"
            android:textColor="@color/azulletras"
            android:textSize="18sp" />

        <TextView
            android:id="@+id/tvIdCliente"
            android:layout_width="180dp"
            android:layout_height="wrap_content"
            android:fontFamily="@font/francois_one"
            android:hint="IdCliente"
            android:textAlignment="center"
            android:textColor="@color/blanco_transparente"
            android:textColorHint="@color/blanco_transparente"
            android:textSize="18sp" />
    </TableRow>

    <TableRow android:layout_width="wrap_content">

        <TextView
            android:id="@+id/ETNEstab"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignTop="@+id/etNomEstb"
            android:layout_alignBottom="@+id/etNomEstb"
            android:fontFamily="@font/francois_one"
            android:gravity="center_vertical"
            android:paddingStart="10dp"
            android:text="Nom. Estab:"
            android:textColor="@color/azulletras"
            android:textSize="16sp" />

        <EditText
            android:id="@+id/etNomEstb"
            android:layout_width="180dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="5dp"
            android:layout_toStartOf="@+id/imgbEstab"
            android:layout_toEndOf="@+id/ETNEstab"
            android:fontFamily="@font/francois_one"
            android:hint="Nombre establecimiento "
            android:paddingStart="10dp"
            android:textColor="@color/blanco_transparente"
            android:textColorHint="@color/blanco_transparente"
            android:textSize="16sp" />

        <ImageButton
            android:id="@+id/imgbEstab"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignTop="@+id/etNomEstb"
            android:layout_alignBottom="@+id/etNomEstb"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_marginEnd="5dp"
            android:background="@android:color/transparent"
            android:scaleType="centerInside"
            app:srcCompat="@drawable/lapiz_opt" />
    </TableRow>

    <TableRow>

        <TextView
            android:id="@+id/ETNProp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignTop="@+id/etNomProp"
            android:layout_alignBottom="@+id/etNomProp"
            android:fontFamily="@font/francois_one"
            android:gravity="center_vertical"
            android:paddingStart="10dp"
            android:text="Nom. Dueño:"
            android:textColor="@color/azulletras"
            android:textSize="16sp" />

        <EditText
            android:id="@+id/etNomProp"
            android:layout_width="180dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="5dp"
            android:layout_toStartOf="@+id/imgbEstab"
            android:layout_toEndOf="@+id/ETNEstab"
            android:fontFamily="@font/francois_one"
            android:hint="Nombre Dueño"
            android:paddingStart="10dp"
            android:textAlignment="viewStart"
            android:textColor="@color/blanco_transparente"
            android:textColorHint="@color/blanco_transparente"
            android:textSize="16sp" />/>


        <ImageButton
            android:id="@+id/imgbPropie"
            android:layout_width="35dp"
            android:layout_height="wrap_content"
            android:layout_alignTop="@+id/etNomProp"
            android:layout_alignBottom="@+id/etNomProp"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_marginEnd="5dp"
            android:background="@android:color/transparent"
            android:scaleType="centerInside"
            app:srcCompat="@drawable/lapiz_opt" />
    </TableRow>

    <TableRow
        android:layout_width="230dp"
        android:layout_height="match_parent">

        <TextView
            android:id="@+id/textView34"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/francois_one"
            android:paddingStart="10dp"
            android:text="Dirección:"
            android:textColor="@color/azulletras"
            android:textSize="18sp" />

        <EditText
            android:id="@+id/etDirecc"
            android:layout_width="180dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="5dp"
            android:layout_toStartOf="@+id/imgbEstab"
            android:layout_toEndOf="@+id/ETNEstab"
            android:fontFamily="@font/francois_one"
            android:hint="Nombre propietario"
            android:paddingStart="10dp"
            android:textAlignment="viewStart"
            android:textColor="@color/blanco_transparente"
            android:textColorHint="@color/blanco_transparente"
            android:textSize="16sp" />

        <ImageButton
            android:id="@+id/imgbDirecc"
            android:layout_width="35dp"
            android:layout_height="35dp"
            android:background="@android:color/transparent"
            android:scaleType="centerInside"
            app:srcCompat="@drawable/lapiz_opt" />
    </TableRow>

    <TableRow>

        <TextView
            android:id="@+id/textView35"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/francois_one"
            android:paddingStart="10dp"
            android:text="Barrio:"
            android:textColor="@color/azulletras"
            android:textSize="18sp" />

        <EditText
            android:id="@+id/etBarrio"
            android:layout_width="180dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="5dp"
            android:layout_toStartOf="@+id/imgbEstab"
            android:layout_toEndOf="@+id/ETNEstab"
            android:fontFamily="@font/francois_one"
            android:hint="Nombre propietario"
            android:paddingStart="10dp"
            android:textAlignment="viewStart"
            android:textColor="@color/blanco_transparente"
            android:textColorHint="@color/blanco_transparente"
            android:textSize="16sp" />

        <ImageButton
            android:id="@+id/imgbBarrio"
            android:layout_width="35dp"
            android:layout_height="35dp"
            android:background="@android:color/transparent"
            android:scaleType="centerInside"
            app:srcCompat="@drawable/lapiz_opt" />
    </TableRow>

    <TableRow>

        <TextView
            android:id="@+id/textView36"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/francois_one"
            android:paddingStart="10dp"
            android:text="Teléfono:"
            android:textColor="@color/azulletras"
            android:textSize="18sp" />

        <EditText
            android:id="@+id/etTelefono"
            android:layout_width="180dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="5dp"
            android:layout_toStartOf="@+id/imgbEstab"
            android:layout_toEndOf="@+id/ETNEstab"
            android:fontFamily="@font/francois_one"
            android:hint="Telefono del establecimiento"
            android:paddingStart="10dp"
            android:textAlignment="viewStart"
            android:textColor="@color/blanco_transparente"
            android:textColorHint="@color/blanco_transparente"
            android:textSize="16sp" />

        <ImageButton
            android:id="@+id/imgbTelefono"
            android:layout_width="35dp"
            android:layout_height="35dp"
            android:background="@android:color/transparent"
            android:scaleType="centerInside"
            app:srcCompat="@drawable/lapiz_opt" />
    </TableRow>

    <TableRow>

        <TextView
            android:id="@+id/textView37"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/francois_one"
            android:paddingStart="10dp"
            android:text="Correo:"
            android:textColor="@color/azulletras"
            android:textSize="18sp" />

        <EditText
            android:id="@+id/etCorreoE"
            android:layout_width="180dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="5dp"
            android:layout_toStartOf="@+id/imgbEstab"
            android:layout_toEndOf="@+id/ETNEstab"
            android:fontFamily="@font/francois_one"
            android:hint="Correo Electronico"

            android:paddingStart="10dp"
            android:textAlignment="viewStart"
            android:textColor="@color/blanco_transparente"
            android:textColorHint="@color/blanco_transparente"
            android:textSize="16sp" />

        <ImageButton
            android:id="@+id/imgbCorreo"
            android:layout_width="35dp"
            android:layout_height="35dp"
            android:background="@android:color/transparent"
            android:scaleType="centerInside"
            app:srcCompat="@drawable/lapiz_opt" />
    </TableRow>

    <TableRow>

        <TextView
            android:id="@+id/textView38"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/francois_one"
            android:paddingStart="10dp"
            android:text="Canal:"
            android:textColor="@color/azulletras"
            android:textSize="18sp" />

        <Spinner
            android:id="@+id/spinnerCanalDC"
            android:layout_width="180dp"
            android:layout_height="35dp"
            android:textColor="@color/blanco_transparente" />

        <ImageButton
            android:id="@+id/imgbCanal"
            android:layout_width="35dp"
            android:layout_height="35dp"
            android:background="@android:color/transparent"
            android:scaleType="centerInside"
            app:srcCompat="@drawable/lapiz_opt" />
    </TableRow>

    <TableRow android:layout_marginBottom="6dp">

        <TextView
            android:id="@+id/textView39"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/francois_one"
            android:paddingStart="10dp"
            android:text="SubCanal:"
            android:textColor="@color/azulletras"
            android:textSize="18sp" />

        <Spinner
            android:id="@+id/spinnerSubCanalDC"
            android:layout_width="180dp"
            android:layout_height="35dp"
            android:textColor="@color/blanco_transparente" />

        <ImageButton
            android:id="@+id/imgbSubC"
            android:layout_width="35dp"
            android:layout_height="35dp"
            android:background="@android:color/transparent"
            android:scaleType="centerInside"
            app:srcCompat="@drawable/lapiz_opt" />
    </TableRow>


</TableLayout>